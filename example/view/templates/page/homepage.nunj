{% extends "layout/main.nunj" %}


{% block content %}
    <h1>Homepage</h1>
    <h2>{{ page.title }}</h2>

    <div>
        <h3>Main Content</h3>
        {{ contentForRegionName("main-content", page) }}
    </div>

    <div>
        <h3>Sidebar Content</h3>
        {{ contentForRegionName("sidebar-content", page) }}
    </div>

    <div>
        <h3>Custom Content</h3>
        {% cmsContent customContent %}
    </div>
{% endblock %}
