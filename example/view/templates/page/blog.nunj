{% extends "layout/main.nunj" %}


{% block content %}
    {#<h2>{{ page.title }}</h2>#}

    <div>
        <h3>Main Content</h3>
        {{ contentForRegionName("main-content", page) }}
    </div>

    <div>
        <h3>List</h3>

        {% for post in posts %}
            <div>
                <h4>{{ post.title }}</h4>
                {{ contentForRegionName("test-container", post) }}
            </div>
        {% endfor %}

    </div>
{% endblock %}
